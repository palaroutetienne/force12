
#conteneur_guitare {
	padding: 0vw;
	background-image: url("../images/guitare_base.png");
	background-repeat: no-repeat;
	background-size: contain;
}
#guitar {
  display: unset;
}
.grid-container {
  display: grid;
  grid-gap: 5px;
  /*background-color: rgb(51, 45, 41);*/
}
.corde {
  display: grid;
  grid-gap: 15px;
  grid-template-columns: 1fr 1fr 1fr 1fr 1fr 1fr 1fr 1fr 1fr 1fr 1fr 1fr 1fr;
  color: #444;
  align-items: center;
  z-index: unset;
}
.grid-container > div .note {
  background-color: rgb(94, 201, 250);
  text-align: center;
  padding: 20px 0px;
  font-size: 10px;
  border-radius: 200px;
  color: #fff;
}
.note{
  z-index: 1;
}
.grid-container > div .vide {
  background-color: rgb(0, 0, 0);
  text-align: center;
  padding: 20px 0px;
  font-size: 12px;
  font-weight: bold;
  color:#fff;
}
.vide{
  z-index: 1;
}
.dessin-corde{
  display: grid;
  position: absolute;
  width: 100%;
  height: 1vh;
  background-color: rgb(151, 151, 151);
  z-index: 0;
}
.fret{
  display: grid;
  grid-gap: 15px;
  grid-template-rows: 1fr 1fr 1fr 1fr 1fr 1fr 1fr 1fr 1fr 1fr 1fr 1fr 1fr;
	padding: 0vw;
  position: absolute;

  width: 1vw;
  height: 70vh;
  background-color: rgb(255, 255, 255);
  z-index: 2;
}